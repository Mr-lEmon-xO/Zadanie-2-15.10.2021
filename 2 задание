#include <iostream>
#include <cmath>
#include <ctime>
using namespace std;
 int main() {
 setlocale(LC_ALL, "Russian");
 srand(time(0));
    cout << "Задание 2" << endl;
		int n;
		long long q, w, e;
		cin >> n;
		e = 0;
		w = 1;
		while (n > 1) {
			q = w;
			w = w + e;
			e = q;
			n = n - 1;
		}
		cout << w << endl; // n-е Числа Фибоначи    
			}
